{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Postgres": "",
    "RabbitMQ": ""
  },
  "Scheduler": {
    "Enabled": true,
    "PollingIntervalSeconds": 60
  },
  "RabbitMqRetry": {
    "MaxRetries": 3,
    "RetryDelays":  ["00:00:01", "00:00:05", "00:00:30"]
  },
  "CircuitBreaker": {
    "ExceptionsAllowedBeforeBreaking": 5,
    "DurationOfBreakMinutes": 1
  },
  "TonApi": {
    "BaseUrl": "https://tonapi.io",
    "ApiKey": "",
    "DefaultLimit": 100
  },
  "Ingestion": {
    "Enabled": true,
    "PollingIntervalSeconds": 30
  },
  "Discovery": {
    "Enabled": true,
    "PollingIntervalMinutes": 60,
    "TopAccountsLimit": 1000
  },
  "Chainstack": {
    "ApiUrl": "https://ton-mainnet.core.chainstack.com"
  }
}
